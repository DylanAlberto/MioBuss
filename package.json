{
  "name": "mi-gym",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && rm -f pnpm-lock.yaml",
    "update-all": "pnpm up --latest && pnpm recursive up --latest"
  },
  "dependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier-config-custom": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "devDependencies": {
    "turbo": "^1.10.9"
  },
  "packageManager": "pnpm@8.4.0"
}
